{
  "name": "@worldsibu/convector-example-dsc-server",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/src/app.js",
  "private": true,
  "scripts": {
    "start": "pm2-runtime pm2.config.json",
    "start:org1:user1": "export USERCERT=user1 ORGCERT=org1 && npm run build && npm run start:daemon",
    "start:org1:user2": "export USERCERT=user2 ORGCERT=org1 && npm run build && npm run start:daemon",
    "start:org1:user3": "export USERCERT=user3 ORGCERT=org1 && npm run build && npm run start:daemon",
    "start:org2:user1": "export USERCERT=user1 ORGCERT=org2 && npm run build && npm run start:daemon",
    "start:org2:user2": "export USERCERT=user2 ORGCERT=org2 && npm run build && npm run start:daemon",
    "start:org2:user3": "export USERCERT=user3 ORGCERT=org2 && npm run build && npm run start:daemon",
    "start:daemon": "pm2 startOrRestart pm2.config.json --no-daemon",
    "stop": "pm2 stop pm2.config.json",
    "tsc": "tsc",
    "clean": "rimraf dist client",
    "refresh": "./node_modules/pm2/bin/pm2 stop 0 && ./node_modules/pm2/bin/pm2 start 0",
    "build": "npm run clean && tsc && cp -R ./src/config ./dist/",
    "build:docker": "docker build -t worldsibu/convector-example-dsc .",
    "build:docker:nocache": "docker build -t worldsibu/convector-example-dsc . --no-cache",
    "test": "mocha -r ts-node/register test/*.spec.ts --reporter spec",
    "userContext": "node -e 'let selfEnroll = require(\"./dist/selfGenContext.js\"); selfEnroll.SelfEnroll.getClient()'"
  },
  "author": "WorldSibu",
  "license": "Apache-2.0",
  "dependencies": {
    "@worldsibu/convector-adapter-fabric": "1.1.8",
    "@worldsibu/convector-example-dsc-cc-drug": "^2.1.0",
    "@worldsibu/convector-storage-couchdb": "1.1.8",
    "@worldsibu/convector-tool-dev-env": "1.1.8",
    "body-parser": "^1.18.3",
    "chai": "^4.1.2",
    "config": "^1.30.0",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "node-couchdb": "^1.3.0",
    "path": "^0.12.7",
    "x509": "^0.3.3"
  },
  "devDependencies": {
    "mocha": "^5.2.0",
    "pm2": "^3.0.0",
    "rimraf": "^2.6.2",
    "ts-node": "^7.0.0",
    "typescript": "^2.9.2"
  }
}
